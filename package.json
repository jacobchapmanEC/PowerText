{
  "name": "Power Text",
  "description": "Create custom keyboard shortcuts to expand and replace text as you type!",
  "author": "Nicholas Hayduk",
  "private": true,
  "license": "BSD",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "globals": {
      "@babel/core": "^7.4.5",
      "@babel/preset-env": "^7.4.5",
      "@babel/register": "^7.4.4",
      "chrome": true
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@eslint/js": "^9.21.0",
    "del": "^4.1.1",
    "eslint": "^9.22.0",
    "globals": "^16.0.0",
    "gulp": "^5.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-chrome-manifest": "^0.0.13",
    "gulp-clean-css": "^4.3.0",
    "gulp-eslint-new": "^2.4.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^6.2.0",
    "gulp-livereload": "^4.0.2",
    "gulp-load-plugins": "^2.0.8",
    "gulp-merge": "^0.1.1",
    "gulp-remove-logging": "^1.2.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-useref": "^3.1.6",
    "gulp-zip": "^5.1.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "jquery": "^3.5.1",
    "run-sequence": "^2.2.1",
    "sinon-chrome": "^3.0.1",
    "wiredep": "^4.0.0"
  },
  "scripts": {
    "test": "jest"
  }
}
